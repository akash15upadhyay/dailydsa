import java.util.ArrayList;

// Defines the structure for each node in the binary tree.
class Node {
    int data;
    Node left;
    Node right;

    // Constructor to create a new node.
    Node(int data) {
        this.data = data;
        this.left = null;
        this.right = null;
    }
}

class Solution {
    ArrayList<Integer> preOrder(Node root) {
        ArrayList<Integer> result = new ArrayList<>();
        preOrderTraversal(root, result);
        return result;
    }
    private void preOrderTraversal(Node root, ArrayList<Integer> result) {
        if (root == null) {
            return;
        }
        result.add(root.data);                   // 1. Process Root
        preOrderTraversal(root.left, result);    // 2. Recurse Left
        preOrderTraversal(root.right, result);   // 3. Recurse Right
    }
}
