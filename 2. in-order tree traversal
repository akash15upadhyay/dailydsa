import java.util.ArrayList;

// Defines the structure for each node in the binary tree.
class Node {
    int data;
    Node left;
    Node right;

    // Constructor to create a new node.
    Node(int data) {
        this.data = data;
        this.left = null;
        this.right = null;
    }
}

class Solution {
    // Main function to start the inorder traversal.
    ArrayList<Integer> inOrder(Node root) {
        ArrayList<Integer> result = new ArrayList<>();
        inOrderTraversal(root, result);
        return result;
    }

    // Recursive helper function to perform the traversal.
    private void inOrderTraversal(Node root, ArrayList<Integer> result) {
        if (root == null) {
            return;
        }
        inOrderTraversal(root.left, result);     // 1. Recurse Left
        result.add(root.data);                   // 2. Process Root
        inOrderTraversal(root.right, result);    // 3. Recurse Right
    }
}
